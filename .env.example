# MongoDB Configuration
ATLAS_URL=mongodb+srv://username:password@cluster.mongodb.net/wanderlust

# Session Secret
SECRET=your_session_secret_key_here

# Cloudinary Configuration (for image uploads)
CLOUD_NAME=your_cloudinary_cloud_name
CLOUD_API_KEY=your_cloudinary_api_key
CLOUD_API_SECRET=your_cloudinary_api_secret

# Mapbox Configuration (for maps and geocoding)
# Get your token from https://www.mapbox.com/
MAP_TOKEN=your_mapbox_access_token

# Razorpay Configuration (Payment Gateway)
# Get these from https://dashboard.razorpay.com/app/keys
RAZORPAY_KEY_ID=rzp_test_xxxxxxxxxxxxxx
RAZORPAY_KEY_SECRET=your_razorpay_secret_key

# Razorpay Webhook Secret
# Get this from https://dashboard.razorpay.com/app/webhooks
RAZORPAY_WEBHOOK_SECRET=your_webhook_secret_here
